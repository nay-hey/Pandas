{
    "manifest_version": 3,
    "name": "Price Scraper Extension",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["http://*/*", "https://*/*"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  